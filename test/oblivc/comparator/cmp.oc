#include<obliv.oh>
#include "neural.h"

void comparator(void* args)
{
  protocolIO *io=args;
  obliv int v1,v2;
  bool eq,lt;
  
  v1 = feedOblivInt(io->val,1);
  v2 = feedOblivInt(io->val,2);
  printf("Current user's value: %d\n", io->val);
  printf("user 1 value : %f\n", v1);
  printf("user 2 value : %f\n", v2);
  revealOblivBool(&eq,v1==v2,0);
  revealOblivBool(&lt,v1<v2,0);
  io->compared = (!eq?(lt?-1:1):0);
}